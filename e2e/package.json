{
  "name": "x-dash-e2e",
  "version": "0.0.0",
  "private": true,
  "description": "This module enables you to write x-dash components that respond to events and change their own data.",
  "keywords": [
    "x-dash"
  ],
  "author": "",
  "license": "ISC",
  "main": "index.js",
  "x-dash": {
    "engine": {
      "server": {
        "runtime": "react",
        "factory": "createElement",
        "component": "Component",
        "fragment": "Fragment",
        "renderModule": "react-dom/server",
        "render": "renderToStaticMarkup"
      },
      "browser": "react"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Financial-Times/x-dash.git"
  },
  "engines": {
    "node": "16.x",
    "npm": "7.x || 8.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@financial-times/x-engine": "file:../packages/x-engine",
    "@financial-times/x-interaction": "file:../components/x-interaction"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "babel-loader": "^8.2.3",
    "check-engine": "^1.10.1",
    "puppeteer": "^10.4.0",
    "webpack": "^5.79.0",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "preinstall": "[ \"$INIT_CWD\" != \"$PWD\" ] || npm_config_yes=true npx check-engine",
    "pretest": "webpack",
    "test": "jest -c jest.e2e.config.js"
  },
  "volta": {
    "extends": "../package.json"
  }
}
