                  viewBox="0 0 36 36"
                >
                  <circle
                    cx="18"
                    cy="18"
                    r="16"
                  />
                  <circle
                    cx="18"
                    cy="18"
                    r="8"
                  />
                </svg>
              </label>
            </div>
          </RadioBtn>
