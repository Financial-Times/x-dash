            checked={true}
            onChange={[MockFunction]}
            value="true"
          >
            <div>
              <input
                checked={true}
                id="ccpa-true"
                name="consent"
                onChange={[MockFunction]}
                type="radio"
                value="true"
              />
              <label
                htmlFor="ccpa-true"
              >
                <span>
                  <strong>
                    Allow
                  </strong>
                  <span>
                    See personalised adverts
                  </span>
                </span>
