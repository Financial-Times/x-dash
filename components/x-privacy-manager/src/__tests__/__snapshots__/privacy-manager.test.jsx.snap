            checked={false}
            onChange={[MockFunction]}
            value="false"
          >
            <div>
              <input
                checked={false}
                id="ccpa-false"
                name="consent"
                onChange={[MockFunction]}
                type="radio"
                value="false"
              />
              <label
                htmlFor="ccpa-false"
              >
                <span>
                  <strong>
                    Block
                  </strong>
                  <span>
                    Only see generic adverts
                  </span>
                </span>
                <svg
                  viewBox="0 0 36 36"
                >
                  <circle
                    cx="18"
                    cy="18"
                    r="16"
                  />
                  <circle
                    cx="18"
                    cy="18"
                    r="8"
                  />
                </svg>
              </label>
            </div>
